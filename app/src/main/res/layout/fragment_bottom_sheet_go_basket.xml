<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenu.BottomSheet.BottomSheetGoBasket">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="80dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/materialButton4"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:background="@drawable/background_button_one"
                android:gravity=""
                android:text="В корзину"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:textAllCaps="false"
                app:backgroundTint="@null"
                app:icon="@drawable/icons"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:elevation="2dp"
                android:text="500 ₽"
                android:textColor="@color/white"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/materialButton4"
                app:layout_constraintEnd_toEndOf="@+id/materialButton4"
                app:layout_constraintTop_toTopOf="@+id/materialButton4" />
        </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>


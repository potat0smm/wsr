<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_bottomsheet_go_basket"
    tools:context=".MainMenu.BottomSheet.BottomSheetGoBasket">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="80dp">
            <com.google.android.material.button.MaterialButton
                android:id="@+id/by_button"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:background="@drawable/background_button_one"
                android:gravity=""
                android:text="В корзину"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:textAllCaps="false"
                app:backgroundTint="@null"
                app:icon="@drawable/icons"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>

